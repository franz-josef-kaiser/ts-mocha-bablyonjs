{
	"compilerOptions": {
		"noEmit": true,
		"module": "commonjs",
		"target": "esnext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"sourceMap": true,
		"allowJs": true,
		"checkJs": true,
		"allowSyntheticDefaultImports": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"lib": [
			"es2020",
			"es2017",
			"es2015",
			"es6"
		]
	}
}